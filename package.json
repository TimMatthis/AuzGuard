{
    "name":  "auzguard",
    "version":  "1.0.0",
    "description":  "Sovereign AI Gateway for Regulated Industries",
    "main":  "dist/server.js",
    "scripts":  {
                    "dev":  "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
                    "dev:backend":  "nodemon --exec ts-node src/server.ts",
                    "dev:frontend":  "npm run dev --prefix frontend",
                    "build":  "npm run build:backend \u0026\u0026 npm run build:frontend",
                    "build:backend":  "tsc",
                    "build:frontend":  "npm run build --prefix frontend",
                    "start":  "node dist/server.js",
                    "test":  "jest",
                    "test:watch":  "jest --watch"
                },
    "dependencies":  {
                         "bcryptjs":  "^2.4.3",
                         "@prisma/client":  "^5.7.1",
                         "ajv":  "^8.12.0",
                         "@fastify/multipart":  "^8.0.0",
                         "zod":  "^3.22.4",
                         "@google/generative-ai":  "^0.6.0",
                         "crypto":  "^1.0.1",
                         "undici":  "^6.19.8",
                         "@fastify/cors":  "^8.4.0",
                         "@fastify/static":  "^6.10.2",
                         "openai":  "^4.10.0",
                         "fastify":  "^4.24.3",
                         "@fastify/jwt":  "^7.2.4"
                     },
    "devDependencies":  {
                            "@types/bcryptjs":  "^2.4.6",
                            "@types/jest":  "^29.5.8",
                            "@types/node":  "^20.9.0",
                            "@types/react":  "^18.2.37",
                            "@types/react-dom":  "^18.2.15",
                            "@vitejs/plugin-react":  "^4.1.1",
                            "concurrently":  "^8.2.2",
                            "jest":  "^29.7.0",
                            "nodemon":  "^3.0.1",
                            "prisma":  "^5.7.1",
                            "react":  "^18.2.0",
                            "react-dom":  "^18.2.0",
                            "react-router-dom":  "^6.18.0",
                            "ts-jest":  "^29.1.1",
                            "ts-node":  "^10.9.1",
                            "typescript":  "^5.2.2",
                            "vite":  "^4.5.0"
                        },
    "keywords":  [
                     "ai",
                     "gateway",
                     "compliance",
                     "sovereignty",
                     "audit"
                 ],
    "author":  "AuzGuard Team",
    "license":  "MIT"
}
